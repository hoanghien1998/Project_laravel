{
  "name": "saritasa/vinhvo",
  "description": "vinhvo",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "barryvdh/laravel-cors": "^0.11",
    "dingo/api": "^2.4",
    "laravel/framework": "^6.0",
    "laravel/horizon": "^3.0",
    "predis/predis": "^1.1",
    "reliese/laravel": "^0.0.16",
    "saritasa/dingo-api-custom": "^2.2",
    "saritasa/eloquent-custom": "^1.1",
    "saritasa/laravel-controllers": "^3.3",
    "saritasa/laravel-fluent-validation": "^1.1",
    "saritasa/laravel-middleware": "^2.1",
    "saritasa/laravel-notifications-api": "^1.2",
    "saritasa/roles-simple": "^1.0",
    "tymon/jwt-auth": "1.0.0-rc.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.2",
    "barryvdh/laravel-ide-helper": "^2.5",
    "beyondcode/laravel-dump-server": "^1.2",
    "nunomaduro/collision": "^2.0",
    "doctrine/dbal": "^2.8",
    "fzaninotto/faker": "^1.7",
    "laravel/tinker": "^2.1",
    "mockery/mockery": "^1.1",
    "phpunit/phpunit": "^7.5",
    "slevomat/coding-standard": "^5.0",
    "arcanedev/log-viewer": "^5.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan horizon:assets",
      "php tools/install-git-hooks.php"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta",
      "php tools/install-git-hooks.php"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "prefer-stable": true
}
